<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FastVideoPlayer" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryVariant">@color/primaryVarient</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondary</item>
        <item name="colorSecondaryVariant">@color/secondaryVarient</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ChipsTheme" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@color/choice_chip_selector</item>
        <item name="strokeColor">@color/primary</item>
        <item name="strokeWidth">5dp</item>
        <item name="android:textColor">@color/chip_text_selector_color</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>
    <style name="cornerImageShap" parent="">
        <item name="cornerSize">20%</item>
    </style>
</resources>